{
  "name": "office-addin-markout",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sierrasoftworks/markout.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "web",
    "dev-server-port": 3000
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead",
          "useBuiltIns": "usage",
          "corejs": {
            "version": "3.9"
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "babel-plugin-transform-class-properties",
      "babel-plugin-syntax-dynamic-import"
    ]
  },
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --mode development --https false",
    "dev-server": "webpack serve",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "start:web:test": "office-addin-debugging start manifest.test.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch",
    "test": "ts-mocha -p tsconfig.json spec/**/*.spec.ts ",
    "test:watch": "ts-mocha -p  tsconfig.json spec/**/*.spec.ts --watch --watch-files src/**/*.ts --watch-files spec/**/*"
  },
  "dependencies": {
    "@types/highlight.js": "^10.1.0",
    "@types/inline-css": "3.0.1",
    "@types/markdown-it": "^12.2.3",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "highlight.js": "^11.3.1",
    "https-browserify": "^1.0.0",
    "inline-css": "^3.0.0",
    "markdown-it": "^12.2.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.3",
    "os-browserify": "^0.3.0",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/chai": "^4.2.22",
    "@types/expect": "^24.3.0",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.0.0",
    "@types/office-js": "^1.0.221",
    "@types/office-runtime": "^1.0.23",
    "babel-loader": "^8.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^18.1.1",
    "mocha": "^9.1.3",
    "office-addin-debugging": "^4.3.5",
    "office-addin-dev-certs": "^1.7.3",
    "office-toolbox": "^0.3.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
