{
  "name": "office-addin-taskpane",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https false",
    "build:dev": "webpack --mode development --https false",
    "build-dev": "webpack --mode development --https false && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "dev-server": "webpack-dev-server --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/inline-css": "0.0.32",
    "@types/markdown-it": "0.0.8",
    "highlight.js": "^9.15.8",
    "inline-css": "^2.4.2",
    "juice": "^5.2.0",
    "markdown-it": "^9.0.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@types/office-js": "^1.0.9",
    "@types/office-runtime": "^1.0.8",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "office-addin-debugging": "^2.3.0",
    "office-addin-dev-certs": "^1.2.4",
    "office-toolbox": "^0.1.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
