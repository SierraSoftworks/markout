{
  "name": "office-addin-markout",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sierrasoftworks/markout.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https true",
    "build:dev": "webpack --mode development --https false",
    "build-dev": "webpack --mode development --https false && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "dev-server": "webpack-dev-server --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "start:web:test": "office-addin-debugging start manifest.test.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch",
    "test": "ts-mocha -p tsconfig.json spec/**/*.spec.ts ",
    "test:watch": "ts-mocha -p  tsconfig.json spec/**/*.spec.ts --watch --watch-files src/**/*.ts --watch-files spec/**/*"
  },
  "dependencies": {
    "@types/highlight.js": "^10.1.0",
    "@types/inline-css": "0.0.33",
    "@types/markdown-it": "0.0.9",
    "highlight.js": "^10.5.0",
    "html2plaintext": "^2.1.2",
    "inline-css": "^3.0.0",
    "juice": "^7.0.0",
    "markdown-it": "^11.0.1",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@types/chai": "^4.2.14",
    "@types/expect": "^24.3.0",
    "@types/jsdom": "^16.2.6",
    "@types/mocha": "^8.2.0",
    "@types/office-js": "^1.0.158",
    "@types/office-runtime": "^1.0.17",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.5.1",
    "jsdom": "^16.4.0",
    "mocha": "^7.2.0",
    "office-addin-debugging": "^3.0.42",
    "office-addin-dev-certs": "^1.5.9",
    "office-toolbox": "^0.3.0",
    "source-map-loader": "^1.1.3",
    "ts-loader": "^8.0.14",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
