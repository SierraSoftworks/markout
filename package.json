{
  "name": "office-addin-markout",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sierrasoftworks/markout.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https true",
    "build:dev": "webpack --mode development --https false",
    "build-dev": "webpack --mode development --https false && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "dev-server": "webpack-dev-server --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "start:web:test": "office-addin-debugging start manifest.test.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/inline-css": "0.0.32",
    "@types/markdown-it": "0.0.9",
    "highlight.js": "^9.18.1",
    "html2plaintext": "^2.1.2",
    "inline-css": "^2.6.3",
    "juice": "^6.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^16.2.1",
    "@types/office-js": "^1.0.85",
    "@types/office-runtime": "^1.0.12",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.3.0",
    "jsdom": "^16.2.2",
    "office-addin-debugging": "^3.0.23",
    "office-addin-dev-certs": "^1.4.15",
    "office-toolbox": "^0.3.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}